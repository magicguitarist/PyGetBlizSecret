    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Attach to Kubernetes Pod (.NET Core)",
            "type": "cloudcode.kubernetes",
            "request": "attach",
            "language": "NETCore",
            "podSelector": {
                "app": "deployment-name"
            },
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "Path to the Remote Directory Containing the Program"
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },

        {
            "name": "Attach Camel Debugger",
            "type": "apache.camel",
            "request": "attach"
        },
        {
            "name": "Attach Camel Debugger",
            "type": "apache.camel",
            "request": "attach"
        },
        "${workspaceFolder}\\development.ini"
        
        "*.json"
        ""
    


     DSN3078.COM   {
         
            "name": "Python: Pyramid 应用",
            "type": "python",
            "request": "launch",
            "module": "pyramid.scripts.pserve",
            "args": [
                "${workspaceFolder}\\development.ini"
            ],
            "pyramid": true,
            "jinja": true,
            "justMyCode": true
        }
    ]
}
